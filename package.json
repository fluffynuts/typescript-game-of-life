{
  "name": "game_of_life",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prestart": "run-s tsc lint test",
    "start": "run-s game",
    "game": "ts-node index.ts",
    "tsc": "tsc",
    "install-packages": "npm install",
    "install-typings": "typings install",
    "prebuild": "run-s install-packages install-typings",
    "build": "tsc",
    "prebublish": "tsc",
    "clean-js": "rimraf js",
    "clean-modules": "rimraf node_modules",
    "clean": "run-p clean-js clean-modules",
    "lint": "tslint src/**/*.ts spec/**/*.ts",
    "pretest": "run-s lint",
    "test": "jest",
    "automocha": "cross-env CONTINUOUS_TESTING=1 jest --watchAll",
    "autolint": "nodemon -q -I -e ts node_modules/tslint/bin/tslint --force ts/src/**/*.ts ts/spec/**/*.ts",
    "autotest": "run-p autojest autolint"
  },
  "author": "Davyd McColl <davydm@gmail.com> (https://github.com/fluffynuts)",
  "license": "ISC",
  "dependencies": {
    "ansi": "^0.3.1",
    "axel": "0.0.3",
    "blessed": "^0.1.81",
    "keypress": "^0.2.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "cross-env": "^7.0.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
